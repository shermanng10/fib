{
 "AWSEBDockerrunVersion": 2,
 "containerDefinitions": [
     {
         "name": "client",
         "image": "shermanng10/multi-client",
         "hostname": "client",
         "essential": "false"
     },
     {
        "name": "nginx",
        "image": "shermanng10/multi-nginx",
        "essential": "true",
        "portMappings": [
            {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["client", "server"]
    },
    {
        "name": "server",
        "image": "shermanng10/multi-server",
        "hostname": "api",
        "essential": "false"
    },
    {
        "name": "worker",
        "image": "shermanng10/multi-worker",
        "hostname": "worker",
        "essential": "false"
    }
 ]   
}